<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClearStartManager.ManagerUiController">
    <children>
        <JFXButton fx:id="toggleAgentButton" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Agent" AnchorPane.leftAnchor="0.0" />
        <JFXButton fx:id="toggleCoachButton" layoutX="100.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Coach" AnchorPane.leftAnchor="100.0">
            <padding>
                <Insets left="20.0" right="20.0" />
            </padding>
        </JFXButton>
        <JFXButton fx:id="saveButton" defaultButton="true" layoutX="400.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Save" AnchorPane.rightAnchor="100.0" />
        <JFXButton fx:id="resetButton" cancelButton="true" layoutX="500.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="100.0" text="Reset" AnchorPane.rightAnchor="0.0" />
      <HBox layoutY="50.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="50.0">
         <children>
            <StackPane>
               <children>
                    <JFXListView fx:id="customerListBox" fixedCellSize="50.0" focusTraversable="false" prefWidth="200.0" />
                  <JFXButton id="addCustomerButton" alignment="CENTER" buttonType="RAISED" opacity="0.8" styleClass="teststyle" text="+" StackPane.alignment="BOTTOM_RIGHT">
                     <StackPane.margin>
                        <Insets bottom="5.0" right="5.0" />
                     </StackPane.margin>
                  </JFXButton>
               </children>
            </StackPane>
              <JFXListView fx:id="settingKeyListBox" fixedCellSize="50.0" focusTraversable="false" prefWidth="250.0" HBox.hgrow="ALWAYS" />
              <JFXListView fx:id="settingValueListBox" fixedCellSize="50.0" focusTraversable="false" prefWidth="250.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
    </children>
</AnchorPane>
